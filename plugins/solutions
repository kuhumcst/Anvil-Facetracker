    + St3^4*St4h*St6*period^2
    + St3^5*St3h*St4*period
    + St3^5*St6*Sth*period
    + -1*St3^6*St4h*period
    )
  *   ( St2*St3*St4*St5*St6^2*period^2
      + -1*St2*St3*St4*St5^2*St7*period^2
      + St2*St3*St4^2*St5*St8*period^2
      + -1*St2*St3*St4^2*St6*St7*period^2
      + -2*St2*St3*St4^4*St5*period
      + 1/2*St2*St4*St5^4*period^2
      + -1*St2*St3^2*St4*St6*St8*period^2
      + St2*St3^2*St4*St7^2*period^2
      + -1*St2*St3^2*St5*St6*St7*period^2
      + 3*St2*St3^2*St4^3*St6*period
      + 1/2*St2*St3^2*St5^2*St8*period^2
      + 1/2*St2*St3^2*St6^3*period^2
      + -3*St2*St3^3*St4^2*St7*period
      + St2*St3^3*St5^3*period
      + 2*St2*St3^4*St4*St8*period
      + -1*St2*St3^4*St5*St7*period
      + -1/2*St2*St3^4*St6^2*period
      + -3/2*St2*St4^2*St5^2*St6*period^2
      + St2*St4^3*St5*St7*period^2
      + 1/2*St2*St4^3*St6^2*period^2
      + -1/2*St2*St4^4*St8*period^2
      + 1/2*St2*St4^6*period
      + -1*St2^2*St3*St4*St5^3*period
      + -1*St2^2*St3*St4^2*St5*St6*period
